@model Mvc1VaccinDemo.ViewModels.SupplierListaInvestViewModel

@{
    ViewData["Title"] = "Vacciner";
}
<section class="py-5">

    

    <div class="row">
        <div class="col-lg-12 mb-5">
            <div class="card">
                <div class="card-header">
                    <h3 class="h6 text-uppercase mb-0">Suppliers under investigation</h3>
                </div>
                <div class="card-body">
                    <table class="table table-hover">
                        <thead>
                        <tr>
                            <th scope="col">Namn</th>
                            <th scope="col">Country</th>
                            <th scope="col"></th>
                        </tr>
                        </thead>
                        <tbody>
                        @foreach (var supplier in Model.Suppliers)
                        {
                            <partial name="_SupplierRow" model="@supplier" />

                        }

                        </tbody>
                    </table>


                </div>
            </div>
        </div>



    </div>
</section>




